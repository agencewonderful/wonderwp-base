            "WonderWp\\Plugin\\Event\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-event\/includes",
            "WonderWp\\Plugin\\Faq\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-faq\/includes",
            "WonderWp\\Plugin\\Galerie\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-galerie\/includes",
            "WonderWp\\Plugin\\GutenbergUtils\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-gutenberg-utils\/includes",
            "WonderWp\\Plugin\\Jeux\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-jeux\/includes",
            "WonderWp\\Plugin\\Map\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-map\/includes",
            "WonderWp\\Plugin\\NewsRoom\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-newsroom\/includes",
            "WonderWp\\Plugin\\Newsletter\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-newsletter\/includes",
            "WonderWp\\Plugin\\Parcours\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-parcours\/includes",
            "WonderWp\\Plugin\\Partenaires\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-partenaires\/includes",
            "WonderWp\\Plugin\\RGPD\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-rgpd\/includes",
            "WonderWp\\Plugin\\Social\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-social\/includes",
            "WonderWp\\Plugin\\Trombinoscope\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-trombinoscope\/includes",
            "WonderWp\\Plugin\\Video\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-video\/includes",
            "WonderWp\\Plugin\\Vote\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-vote\/includes",
            "WonderWp\\Plugin\\Cron\\": "web\/app\/plugins\/wwp-cron\/includes",
            "WonderWp\\Plugin\\Actu\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-actu\/includes",
            "WonderWp\\Plugin\\Slider\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-slider\/includes",
            "WonderWp\\Plugin\\Search\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-search\/includes",
            "WonderWp\\Plugin\\Stats\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-stats\/includes",
            "WonderWp\\Plugin\\Tracker\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-tracker\/includes",
            "WonderWp\\Plugin\\Translator\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-translator\/includes",
            "WonderWp\\Plugin\\Download\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-download\/includes",
            "WonderWp\\Plugin\\Alerte\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-alerte\/includes"
        }
    },
    "extra": {
        "installer-paths": {
            "web\/app\/mu-plugins\/{$name}\/": [
                "type:wordpress-muplugin"
            ],
            "web\/app\/plugins\/{$name}\/": [
                "type:wordpress-plugin"
            ],
            "web\/app\/themes\/{$name}\/": [
                "type:wordpress-theme"
            ]
        },
        "wordpress-install-dir": "web\/wp"
    },
    "scripts": {
        "test": [
            "vendor\/bin\/phpcs --ignore=web\/wp\/,vendor\/ -n -s ."
        ]
    }
}
