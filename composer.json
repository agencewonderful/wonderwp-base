{
    "name": "wonderwp\/site",
    "type": "project",
    "license": "MIT",
    "description": "A modern WordPress stack",
    "homepage": "https:\/\/roots.io\/bedrock\/",
    "authors": [{"name": "Jeremy Desvaux", "email": "jeremy.desvaux@wonderful.fr", "homepage": "https:\/\/github.com\/jeremy-wdf"}],
    "keywords": ["bedrock", "roots", "wordpress", "stack", "composer", "vagrant", "wp"],
    "config": {"preferred-install": {"wonderwp\/*": "source", "agencewonderful\/*": "source", "*": "dist"}},
    "repositories": [
        {"type": "composer", "url": "https:\/\/wpackagist.org"},
        {"type": "vcs", "url": "https:\/\/github.com\/jeremy-wdf\/daux.io"},
        {"type": "composer", "url": "https:\/\/won.wonderful.fr\/satis\/packages-mirror"}
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6",
        "composer\/installers": "~1.0.12",
        "vlucas\/phpdotenv": "^2.0.1",
        "oscarotero\/env": "^1.0",
        "johnpbloch\/wordpress": "^4.7",
        "roots\/wp-password-bcrypt": "1.0.0",
        "wpackagist-plugin\/cms-tree-page-view": "dev-trunk",
        "agencewonderful\/wonderwp-theme-core": "dev-feature/wwp_v2@dev",
        "agencewonderful\/wwp-actu": "dev-feature/wwp_v2@dev",
        "agencewonderful\/wwp-branchepage": "dev-feature/wwp_v2@dev",
        "agencewonderful\/wwp-contact": "dev-feature/wwp_v2@dev",
        "agencewonderful\/wwp-download": "dev-feature/wwp_v2@dev",
        "agencewonderful\/wwp-emploi": "dev-feature/wwp_v2@dev",
        "agencewonderful\/wwp-espace-restreint": "dev-feature/wwp_v2@dev",
        "agencewonderful\/wwp-faq": "dev-feature/wwp_v2@dev",
        "agencewonderful\/wwp-jeux": "dev-feature/wwp_v2@dev",
        "agencewonderful\/wwp-map": "dev-feature/wwp_v2@dev",
        "agencewonderful\/wwp-meteo": "dev-feature/wwp_v2@dev",
        "agencewonderful\/wwp-newsletter": "dev-feature/wwp_v2@dev",
        "agencewonderful\/wwp-social": "dev-feature/wwp_v2@dev",
        "agencewonderful\/wwp-stats": "dev-feature/wwp_v2@dev",
        "agencewonderful\/wwp-trombinoscope": "dev-feature/wwp_v2@dev",
        "agencewonderful\/wwp-translator": "dev-feature/wwp_v2@dev",
        "agencewonderful\/wwp-vote": "dev-feature/wwp_v2@dev",
        "twig\/twig": "^2.0",
        "agencewonderful\/wwp-lazyload": "dev-feature/wwp_v2@dev"
    },
    "require-dev": {"wpackagist-plugin\/debug-bar": "dev-trunk", "daux\/daux.io": "0.7.2"},
    "autoload": {
        "psr-4": {
            "WonderWp\\Theme\\Child\\": "web\/app\/themes\/wwp_child_theme\/includes",
            "WonderWp\\Plugin\\Jeux\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-jeux\/includes",
            "WonderWp\\Plugin\\Contact\\Child\\": "web\/app\/themes\/wwp_child_theme\/plugins\/wwp-contact\/includes"
        }
    },
    "extra": {
        "installer-paths": {
            "web\/app\/mu-plugins\/{$name}\/": ["type:wordpress-muplugin"],
            "web\/app\/plugins\/{$name}\/": ["type:wordpress-plugin"],
            "web\/app\/themes\/{$name}\/": ["type:wordpress-theme"]
        },
        "wordpress-install-dir": "web\/wp"
    },
    "scripts": {"test": ["vendor\/bin\/phpcs --ignore=web\/wp\/,vendor\/ -n -s ."]}
}
